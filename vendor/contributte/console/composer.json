{
  "name": "contributte/console",
  "description": "Best Symfony Console for Nette Framework",
  "keywords": [
    "nette",
    "symfony",
    "console"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/console",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "nette/di": "^3.1.8",
    "symfony/console": "^6.4.2 || ^7.0.2"
  },
  "require-dev": {
    "nette/http": "^3.2.3",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.4",
    "contributte/phpstan": "^0.1",
    "mockery/mockery": "^1.6.7",
    "symfony/event-dispatcher": "^6.4.2 || ^7.0.2"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Console\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.11.x-dev"
    }
  }
}
